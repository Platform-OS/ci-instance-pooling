{% liquid 
  assign providedDomain = object.domain
  assign domain = object.domain | remove_first: "https://" | remove_last: "/"
  assign pos_cli_token = object.pos_cli_token
%}

{% parse_json object %}
  {
    "domain": {{ domain | json }},
    "providedDomain": {{ providedDomain | json }},
    "pos_cli_token": {{ pos_cli_token | json }},
    "available": {{ true | json  }}
  }
{% endparse_json %}
  
{% return object %}